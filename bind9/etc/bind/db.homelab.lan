$TTL 24h
$ORIGIN homelab.lan.
@           IN      SOA     ns1.homelab.lan. admin.homelab.lan. (
                                1               ; serial number
		                        24h			    ; refresh period
		                        2h			    ; retry period
		                        1000h			; expire time
		                        2d   			; negative TTL
                                )

@           IN      NS      ns1.homelab.lan.

ns1		    IN      A	    192.168.102.30

; Main VLAN network services
pvedemo1    IN      A       192.168.102.10
pvedemo2    IN      A       192.168.102.11
pvedemo3    IN      A       192.168.102.12
ns1         IN      A       192.168.102.30
host        IN      A       192.168.102.30