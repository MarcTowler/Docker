$TTL 24h
$ORIGIN homelab.lan.
@           IN      SOA     ns1.homelab.lan. admin.homelab.lan. (
                                1               ; serial number
		                        24h			    ; refresh period
		                        2h			    ; retry period
		                        1000h			; expire time
		                        2d   			; negative TTL
                                )

@           IN      NS      ns1.homelab.lan.

ns1		    IN      A	    192.168.1.146

; Main VLAN network services
db			IN		A		192.168.1.144
api         IN      A       192.168.1.145
gapi	    IN      A       192.168.1.147
rpg		    IN      A       192.168.1.148
site		IN		A		192.168.1.149
ns1         IN      A       192.168.1.146
host        IN      A       192.168.1.146
socket		IN		A		192.168.1.151
runner		IN		A		192.168.1.152